<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mybatisdemo.login.mapper.IndexMapper">

    <select id="listAreaMybatis" resultType="cn.mybatisdemo.login.entity.DTO.AreaDTO">
        SELECT area_id AS areaId,area_level AS areaLevel,area_name AS areaName,
        area_parent AS areaParent FROM areas
    </select>

</mapper>